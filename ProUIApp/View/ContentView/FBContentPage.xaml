<UserControl x:Class="ProUIApp.View.ContentView.FBContentPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProUIApp.View.ContentView"
             xmlns:dragablz="http://dragablz.net/winfx/xaml/dragablz"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
      
        <dragablz:TabablzControl Name="TabControlAccount">
            <TabItem Header="Facebook Main">
                <ContentControl Content="{Binding FBMainPage.Value}"  />
            </TabItem>
            <TabItem Header="Facebook Media">
                <ContentControl Content="{Binding FBMediaPage.Value}"  />
            </TabItem>
        </dragablz:TabablzControl>

    </Grid>
</UserControl>
